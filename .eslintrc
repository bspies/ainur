{
    "extends": "eslint-config-airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "mocha": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "comma-dangle": 0,
        "class-methods-use-this": 0,
        "import/no-dynamic-require": 0,
        "linebreak-style": 0,
        "no-console": 0
    }
}